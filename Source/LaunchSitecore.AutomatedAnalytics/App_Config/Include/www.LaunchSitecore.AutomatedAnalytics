  <sitecore>
    <settings>
      <setting name="Analytics.AutoDetectBots">
        <patch:attribute name="value">false</patch:attribute>"
      </setting>
      <setting name="Analytics.Robots.IgnoreRobots">
        <patch:attribute name="value">false</patch:attribute>"
      </setting>
    </settings>
  </sitecore>
</configuration>