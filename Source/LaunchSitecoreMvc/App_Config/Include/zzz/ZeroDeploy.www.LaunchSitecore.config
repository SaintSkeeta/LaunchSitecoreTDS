<configuration xmlns:x="http://www.sitecore.net/xmlconfig/">
  <sitecore>

    <commands>
      <!-- Ribbon commands for the security window to enable and reset the passwords. -->
      <command name="launchsitecore:createsecurityaccounts" type="LaunchSitecore.Configuration.Security.CreateSecurityAccounts,LaunchSitecore">
        <x:attribute name="type" value="LaunchSitecore.Configuration.Security.CreateSecurityAccounts" />
        <x:attribute name="ZeroDeploy" value="true" />
      </command>
    </commands>

    <pipelines>
      <initialize>
        <processor type="LaunchSitecore.App_Start.GlassMapperSc, LaunchSitecore">
          <x:attribute name="ZeroDeploy" value="true" />
        </processor>
      </initialize>
    </pipelines>
  </sitecore>
</configuration>