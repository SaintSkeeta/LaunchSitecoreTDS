@model LaunchSitecore.Models.VisitInformation
<div id="pnlDetails">
    <h3 class="center-title">
        @UIHelpers.GetDictionaryText("Visit Details")
        <span class="pagecounts">
            <img src="/assets/img/page.png" alt="Pages" title="Pages Viewed" />@Model.PageCount
            <img src="/assets/img/value.png" alt="Value" title=" Engagement Value" />@Model.EngagementValue
        </span>
    </h3>
    <div style="clear: both"></div>
    <div id="accordion" class="panel-group">
        <div class="panel panel-default">
            <div class="panel-heading">
                <a href="#collapseOne" data-parent="#accordion" data-toggle="collapse" class="panel-title collapsed">
                    @UIHelpers.GetDictionaryText("Pattern")
                </a>
            </div>
            <div id="collapseOne" class="panel-collapse collapse">
                <div class="panel-body">
                    <div class="patternwrapper">
                        @if (Model.PatternMatches.Count == 0)
                        {
                            <div>
                                <div class="image-wrapper"><img src="/assets/img/nope.png" alt="Unknown" /></div>
                                <h4>@UIHelpers.GetDictionaryText("No Match")</h4>
                                <h5>@UIHelpers.GetDictionaryText("DMS Session Info No Pattern Match Name")</h5>
                            </div>
                            <div style="clear: both"></div>
                        }
                        else
                        {
                            foreach (var item in Model.PatternMatches)
                            {
                            <div>
                                <div class="image-wrapper"><img src="@item.Image" alt="@item.PatternName" /></div>
                                <h4>@item.Profile</h4>
                                <h5>@item.PatternName</h5>
                            </div>
                            <div style="clear: both"></div>
                            }
                        }
                    </div>
                </div>
            </div>
        </div>
        <div class="panel panel-default">
            <div class="panel-heading">
                <a href="#collapseTwo" data-parent="#accordion" data-toggle="collapse" class="panel-title collapsed">
                    @UIHelpers.GetDictionaryText("Campaign")
                </a>
            </div>
            <div class="panel-collapse collapse" id="collapseTwo">
                <div class="panel-body">
                    <ul>
                        <li>@Model.Campaign</li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="panel panel-default">
            <div class="panel-heading">
                <a href="#collapseThree" data-parent="#accordion" data-toggle="collapse" class="panel-title collapsed">
                    @UIHelpers.GetDictionaryText("GeoIp Location")
                </a>
            </div>
            <div class="panel-collapse collapse" id="collapseThree">
                <div class="panel-body">
                    <ul>
                        <li>@UIHelpers.GetDictionaryText("GeoIp City"): @Model.City</li>
                        <li>@UIHelpers.GetDictionaryText("GeoIp Postal Code"): @Model.PostalCode</li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="panel panel-default">
            <div class="panel-heading">
                <a href="#collapseFour" data-parent="#accordion" data-toggle="collapse" class="panel-title collapsed">
                    @UIHelpers.GetDictionaryText("Pages")
                </a>
            </div>
            <div class="panel-collapse collapse" id="collapseFour">
                <div class="panel-body">
                    <div style="overflow-y:auto; overflow-x:hidden; max-height:200px; margin-left: -10px; margin-right: -25px;">
                        <ul>
                            @foreach (var item in Model.PagesViewed)
                            {
                                <li><a href="@item.Url">@item.Title</a></li>
                            }
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="panel panel-default">
            <div class="panel-heading">
                <a href="#collapseFive" data-parent="#accordion" data-toggle="collapse" class="panel-title collapsed">
                    @UIHelpers.GetDictionaryText("Goals")
                </a>
            </div>
            <div class="panel-collapse collapse" id="collapseFive">
                <div class="panel-body">
                    <ul>
                        @foreach (var item in Model.GoalsList)
                                {
                                <li>@item</li>
                                }
                    </ul>
                </div>
            </div>
        </div>
        <div class="panel panel-default">
            <div class="panel-heading">
                <a href="#collapseSix" data-parent="#accordion" data-toggle="collapse" class="panel-title collapsed">
                    @UIHelpers.GetDictionaryText("Engagement")
                </a>
            </div>
            <div class="panel-collapse collapse" id="collapseSix">
                <div class="panel-body">
                    @if (Model.EngagementStates.Any())
                    {
                    <ul>
                        @foreach (var item in Model.EngagementStates)
                        {
                                <li>Engagment Plan Title: @item.EngagementPlanTitle , Title: @item.Title , Date: @item.Date</li>
                        }
                    </ul>
                    }
                    else
                    {
                        <ul><li>Unable to load Engagement States</li></ul>
                    }
                </div>
            </div>
        </div>
    </div>
</div>